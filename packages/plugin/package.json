    "@types/node": "^20",
    "@vitest/coverage-v8": "^0.34.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.0.0",
    "tsup": "^7.0.0",
    "typescript": "^5.0.0",
    "vitest": "^0.34.0"
  }
