{
  "name": "@rpxl/recast",
  "license": "MIT",
  "version": "1.0.7",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.esm.js",
      "require": "./dist/core.cjs",
      "default": "./dist/core.modern.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.esm.js",
      "require": "./dist/client.cjs",
      "default": "./dist/client.modern.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.esm.js",
      "require": "./dist/server.cjs",
      "default": "./dist/server.modern.js"
    },
    "./dist/core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.esm.js",
      "require": "./dist/core.cjs",
      "default": "./dist/core.modern.js"
    },
    "./dist/client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.esm.js",
      "require": "./dist/client.cjs",
      "default": "./dist/client.modern.js"
    },
    "./dist/server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.esm.js",
      "require": "./dist/server.cjs",
      "default": "./dist/server.modern.js"
    },
    "./dist/core/types": {
      "types": "./dist/core/types.d.ts"
    },
    "./dist/client/types": {
      "types": "./dist/client/types.d.ts"
    },
    "./dist/server/types": {
      "types": "./dist/server/types.d.ts"
    }
  },
  "files": [
    "dist",
    "core",
    "client",
    "server"
  ],
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "microbundle src/{core,client,server}.ts --jsx React.createElement",
    "dev": "microbundle watch src/{core,client,server}.ts --jsx React.createElement",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "rimraf": "^5.0.1"
  }
}
