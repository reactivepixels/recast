{
  "name": "@rpxl/recast",
  "license": "MIT",
  "version": "1.0.6",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.esm.js",
      "require": "./dist/core.cjs",
      "default": "./dist/core.modern.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.esm.js",
      "require": "./dist/client.cjs",
      "default": "./dist/client.modern.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.esm.js",
      "require": "./dist/server.cjs",
      "default": "./dist/server.modern.js"
    },
    "./dist/core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.esm.js",
      "require": "./dist/core.cjs",
      "default": "./dist/core.modern.js"
    },
    "./dist/client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.esm.js",
      "require": "./dist/client.cjs",
      "default": "./dist/client.modern.js"
    },
    "./dist/server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.esm.js",
      "require": "./dist/server.cjs",
      "default": "./dist/server.modern.js"
    },
    "./dist/core/types": {
      "types": "./dist/core/types.d.ts"
    },
    "./dist/client/types": {
      "types": "./dist/client/types.d.ts"
    },
    "./dist/server/types": {
      "types": "./dist/server/types.d.ts"
    }
  },
  "files": [
    "dist",
    "core",
    "client",
    "server"
  ],
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "scripts": {
    "build": "microbundle src/{core,client,server}.ts --jsx React.createElement",
    "dev": "microbundle watch src/{core,client,server}.ts --jsx React.createElement",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "microbundle": "^0.15.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "5.1.6"
  }
}
