{
  "name": "@rpxl/recast-primitives",
  "license": "MIT",
  "version": "1.0.7",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.esm.js",
      "require": "./dist/client.cjs",
      "default": "./dist/client.modern.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.esm.js",
      "require": "./dist/server.cjs",
      "default": "./dist/server.modern.js"
    }
  },
  "files": [
    "dist",
    "client",
    "server"
  ],
  "peerDependencies": {
    "@rpxl/recast": "*",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "scripts": {
    "build": "microbundle src/{client,server}.ts --globals @rpxl/recast/client=recastClient,@rpxl/recast/server=recastServer --jsx React.createElement",
    "dev": "microbundle watch src/{client,server}.ts --globals @rpxl/recast/client=recastClient,@rpxl/recast/server=recastServer --jsx React.createElement"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@rpxl/recast": "*",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "microbundle": "^0.15.1",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "classnames": "^2.3.2"
  }
}
