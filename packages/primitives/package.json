{
  "name": "@rpxl/recast-primitives",
  "license": "MIT",
  "version": "1.0.11",
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.module.mjs",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/index.js",
    "default": "./dist/index.modern.mjs"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@rpxl/recast": "*",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "microbundle src/index.ts --globals @rpxl/recast/client=recastClient,@rpxl/recast/server=recastServer --jsx React.createElement",
    "dev": "microbundle watch src/index.ts --globals @rpxl/recast/client=recastClient,@rpxl/recast/server=recastServer --jsx React.createElement"
  },
  "devDependencies": {
    "@rpxl/recast": "*",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "microbundle": "^0.15.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "react-aria": "^3.28.0",
    "rimraf": "^5.0.1",
    "tailwind-merge": "^2.0.0"
  }
}
