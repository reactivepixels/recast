{
  "name": "@rpxl/recast-primitives",
  "license": "MIT",
  "version": "1.0.11",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.modern.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./accordion": {
      "types": "./dist/accordion.d.ts",
      "import": "./dist/accordion.esm.js",
      "require": "./dist/accordion.cjs",
      "default": "./dist/accordion.modern.js"
    },
    "./alert": {
      "types": "./dist/alert.d.ts",
      "import": "./dist/alert.esm.js",
      "require": "./dist/alert.cjs",
      "default": "./dist/alert.modern.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.modern.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@rpxl/recast": "*",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "microbundle src/{index,alert,accordion}.ts --jsx React.createElement",
    "dev": "microbundle watch src/index.ts --globals --jsx React.createElement"
  },
  "devDependencies": {
    "@rpxl/recast": "*",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "microbundle": "^0.15.1",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.0.2",
    "clsx": "^2.0.0",
    "react-aria": "^3.29.1",
    "rimraf": "^5.0.5",
    "tailwind-merge": "^2.0.0"
  }
}
