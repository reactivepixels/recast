{
  "name": "@rpxl/recast-monorepo",
  "version": "0.1.0",
  "license": "MIT",
  "workspaces": [
    "packages/docs",
    "packages/lib",
    "packages/marketing",
    "packages/primitives"
  ],
  "scripts": {
    "build:marketing": "npm run build -w packages/marketing",
    "build:recast": "npm run build -w packages/lib",
    "build:recast-primitives": "npm run build -w packages/primitives",
    "build:all": "npm run build:recast && npm run build:recast-primitives",
    "dev:docs": "npm run dev -w packages/docs",
    "dev:marketing": "npm run dev -w packages/marketing",
    "export:marketing": "npm run export -w packages/marketing",
    "test:ci": "npm run test:coverage -w packages/lib",
    "release": "npm run build:all && npx changeset publish"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "cmdk": "^0.2.0"
  }
}
